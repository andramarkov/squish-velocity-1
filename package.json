{
  "name": "squish_velocity",
  "version": "1.0.2",
  "description": "A two-stroke cylinder head squish velocity calculator",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --executable-name \"Squish velocity calculator\" --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager .  --executable-name \"Squish velocity calculator\" --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Squish velocity calculator\"",
    "package-linux": "electron-packager . --executable-name \"Squish velocity calculator\" --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release-builds"
  },
  "author": "Elmeri Laakkonen",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "bootstrap-validate": "^2.1.3",
    "chart.js": "^2.7.2",
    "electron": "^2.0.4",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}